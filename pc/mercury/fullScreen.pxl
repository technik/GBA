#version 330 core
out vec4 FragColor;

in vec2 uv0;

uniform usampler2D backBuffer;

void main()
{
	int clr15 = int(texture(backBuffer, uv0).x);
	float red = (clr15&0x1f) / 63.0;
	float green = ((clr15>>5)&0x1f) / 63.0;
	float blue = ((clr15>>10)&0x1f) / 63.0;
    FragColor = vec4(red, green, blue, 1.0);
}